{
  "Routes": [
    //perflow
    {
      "DownstreamPathTemplate": "/api/Playlists",
      "DownstreamScheme": "https",
      "DownstreamHostandPorts": [
        {
          "Host": "localhost",
          "Port": "5001"
        }

      ],
      "UpstreamPathTemplate": "/api/Playlists",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT" ]
    },
    {
      "DownstreamPathTemplate": "/api/Playlists/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostandPorts": [
        {
          "Host": "localhost",
          "Port": "5001"
        }

      ],
      "UpstreamPathTemplate": "/api/Playlists/{id}",
      "UpstreamHttpMethod": [ "GET", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/Albums/new-releases",
      "DownstreamScheme": "https",
      "DownstreamHostandPorts": [
        {
          "Host": "localhost",
          "Port": "5001"
        }

      ],
      "UpstreamPathTemplate": "/api/Albums/new-releases",
      "UpstreamHttpMethod": [ "GET" ]
    },
    //perflow.studio
    {
      "DownstreamPathTemplate": "/api/songs",
      "DownstreamScheme": "https",
      "DownstreamHostandPorts": [
        {
          "Host": "localhost",
          "Port": "44355"
        }
      ],
      "UpstreamPathTemplate": "/api/songs",
      "UpstreamHttpMethod": [ "GET", "POST" ]
    },
    {
      "DownstreamPathTemplate": "/api/songs/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostandPorts": [
        {
          "Host": "localhost",
          "Port": "44355"
        }
      ],
      "UpstreamPathTemplate": "/api/songs/{id}",
      "UpstreamHttpMethod": [ "GET", "PUT", "DELETE" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:44328"
  }
}  

